            </class-path-extension>
            <class-path-extension>
                <runtime-relative-path>ext/xml-apis.jar</runtime-relative-path>
                <binary-origin>release/modules/ext/xml-apis.jar</binary-origin>
            </class-path-extension>
            <class-path-extension>
                <runtime-relative-path>ext/serializer.jar</runtime-relative-path>
