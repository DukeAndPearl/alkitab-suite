                <attr name="displayName" bundlevalue="kiyut.alkitab.options.GeneralOptionsPanel#DisplayName"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.spi.options.AdvancedOption.createSubCategory"/>
                <attr name="keywords" bundlevalue="kiyut.alkitab.options.GeneralOptionsPanel#Keywords"/>
                <attr name="keywordsCategory" stringvalue="BookViewerOptionsCategory/null"/>
                <attr name="toolTip" bundlevalue="kiyut.alkitab.options.GeneralOptionsPanel#Tooltip"/>
            </file>
            <file name="kiyut-alkitab-options-ViewerHintsAdvancedOptions.instance">
                <attr name="position" intvalue="100"/>
                <attr name="controller" newvalue="kiyut.alkitab.options.ViewerHintsOptionsPanelController"/>
                <attr name="displayName" bundlevalue="kiyut.alkitab.options.ViewerHintsOptionsPanel#DisplayName"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.spi.options.AdvancedOption.createSubCategory"/>
                <attr name="keywords" bundlevalue="kiyut.alkitab.options.ViewerHintsOptionsPanel#Keywords"/>
