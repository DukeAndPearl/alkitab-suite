<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="File">
            <file name="kiyut-alkitab-actions-NewViewAction.instance">
                <attr name="instanceClass" stringvalue="kiyut.alkitab.actions.NewViewAction"/>
            </file>
        </folder>
        <folder name="Window">
            <file name="SelectDocumentNode_hidden"/>
            <file name="kiyut-alkitab-actions-BookshelfAction.instance">
                <attr name="instanceClass" stringvalue="kiyut.alkitab.actions.BookshelfAction"/>
            </file>
            <file name="kiyut-alkitab-actions-BookViewerAction.instance">
                <attr name="instanceClass" stringvalue="kiyut.alkitab.actions.BookViewerAction"/>
            </file>
            <file name="kiyut-alkitab-actions-BookNavigatorAction.instance">
                <attr name="instanceClass" stringvalue="kiyut.alkitab.actions.BookNavigatorAction"/>
            </file>
            <file name="kiyut-alkitab-actions-DailyDevotionsAction.instance">
                <attr name="instanceClass" stringvalue="kiyut.alkitab.actions.DailyDevotionsAction"/>
            </file>
            <file name="kiyut-alkitab-actions-DefinitionsAction.instance">
                <attr name="instanceClass" stringvalue="kiyut.alkitab.actions.DefinitionsAction"/>
            </file>
        </folder>
    </folder>
    <folder name="OptionsDialog">
        <file name="BookViewerOptionCategory.instance">
            <attr name="instanceClass" stringvalue="kiyut.alkitab.options.BookViewerOptionsCategory"/>
        </file>
    </folder>
    <folder name="Services">
        <folder name="Alkitab">
            <file name="CoreBookViewProvider.instance">
                <attr name="instanceClass" stringvalue="kiyut.alkitab.core.CoreBookViewProvider"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="BookNavigatorTopComponent.settings" url="windows/BookNavigatorTopComponent.settings"/>
            <file name="BookshelfTopComponent.settings" url="windows/BookshelfTopComponent.settings"/>
            <!-- <file name="DefinitionsTopComponent.settings" url="windows/DefinitionsTopComponent.settings"/>
            <file name="DailyDevotionsTopComponent.settings" url="windows/DailyDevotionsTopComponent.settings"/>
            -->
        </folder> 
        <folder name="Modes">
            <folder name="explorer">
                <file name="BookshelfTopComponent.wstcref" url="windows/BookshelfTopComponent.wstcref">
                    <attr name="position" intvalue="100"/>
                 </file>
                <file name="BookNavigatorTopComponent.wstcref" url="windows/BookNavigatorTopComponent.wstcref">
                    <attr name="position" intvalue="110"/>
                 </file>
            </folder>
            <folder name="output">
                <file name="DefinitionsTopComponent.wstcref" url="windows/DefinitionsTopComponent.wstcref">
                    <attr name="position" intvalue="200"/>
                 </file>
                <file name="DailyDevotionsTopComponent.wstcref" url="windows/DailyDevotionsTopComponent.wstcref">
                    <attr name="position" intvalue="210"/>
                </file>
            </folder>
        </folder>
    </folder>
</filesystem>
