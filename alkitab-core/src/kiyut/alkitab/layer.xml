                <attr name="keywordsCategory" stringvalue="BookViewerOptionsCategory/null"/>
                <attr name="toolTip" bundlevalue="kiyut.alkitab.options.PathOptionsPanel#Tooltip"/>
            </file>
        </folder>
        <file name="BookViewerOptionsCategory.instance">
            <attr name="position" intvalue="0"/>
            <attr name="advancedOptionsFolder" stringvalue="OptionsDialog/BookViewerOptionsCategory"/>
            <attr name="categoryName" bundlevalue="kiyut.alkitab.options.Bundle#BookViewerOptionsCategory.Name"/>
            <attr name="iconBase" bundlevalue="kiyut.alkitab.options.Bundle#BookViewerOptionsCategory.IconBase"/>
            <attr name="instanceCreate" methodvalue="org.netbeans.spi.options.OptionsCategory.createCategory"/>
            <attr name="keywords" bundlevalue="kiyut.alkitab.options.Bundle#BookViewerOptionsCategory.Keywords"/>
            <attr name="keywordsCategory" bundlevalue="kiyut.alkitab.options.Bundle#BookViewerOptionsCategory.KeywordsCategory"/>
            <attr name="title" bundlevalue="kiyut.alkitab.options.Bundle#BookViewerOptionsCategory.Title"/>
        </file>
    </folder>
    <folder name="Services">
        <folder name="Alkitab">
            <file name="CoreBookViewProvider.instance">
                <attr name="instanceClass" stringvalue="kiyut.alkitab.core.CoreBookViewProvider"/>
            </file>
        </folder>
    </folder>    
</filesystem>
