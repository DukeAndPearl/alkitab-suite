<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="kiyut-alkitab-windows-TestTopComponent.instance_hidden"/>
        </folder>
    </folder>
    <folder name="OptionsDialog">
        <folder name="BookViewerOptionsCategory">
            <file name="kiyut-alkitab-options-GeneralAdvancedOptions.instance">
                <attr name="position" intvalue="0"/>
                <attr name="controller" newvalue="kiyut.alkitab.options.GeneralOptionsPanelController"/>
                <attr name="displayName" bundlevalue="kiyut.alkitab.options.GeneralOptionsPanel#DisplayName"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.spi.options.AdvancedOption.createSubCategory"/>
                <attr name="keywords" bundlevalue="kiyut.alkitab.options.GeneralOptionsPanel#Keywords"/>
                <attr name="keywordsCategory" stringvalue="BookViewerOptionsCategory/null"/>
                <attr name="toolTip" bundlevalue="kiyut.alkitab.options.GeneralOptionsPanel#Tooltip"/>
            </file>
            <file name="kiyut-alkitab-options-ViewerHintsAdvancedOptions.instance">
                <attr name="position" intvalue="100"/>
                <attr name="controller" newvalue="kiyut.alkitab.options.ViewerHintsOptionsPanelController"/>
                <attr name="displayName" bundlevalue="kiyut.alkitab.options.ViewerHintsOptionsPanel#DisplayName"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.spi.options.AdvancedOption.createSubCategory"/>
                <attr name="keywords" bundlevalue="kiyut.alkitab.options.ViewerHintsOptionsPanel#Keywords"/>
                <attr name="keywordsCategory" stringvalue="BookViewerOptionsCategory/null"/>
                <attr name="toolTip" bundlevalue="kiyut.alkitab.options.ViewerHintsOptionsPanel#Tooltip"/>
            </file>
            <file name="kiyut-alkitab-options-PathAdvancedOptions.instance">
                <attr name="position" intvalue="200"/>
                <attr name="controller" newvalue="kiyut.alkitab.options.PathOptionsPanelController"/>
                <attr name="displayName" bundlevalue="kiyut.alkitab.options.PathOptionsPanel#DisplayName"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.spi.options.AdvancedOption.createSubCategory"/>
                <attr name="keywords" bundlevalue="kiyut.alkitab.options.PathOptionsPanel#Keywords"/>
                <attr name="keywordsCategory" stringvalue="BookViewerOptionsCategory/null"/>
                <attr name="toolTip" bundlevalue="kiyut.alkitab.options.PathOptionsPanel#Tooltip"/>
            </file>
        </folder>
        <file name="BookViewerOptionsCategory.instance">
            <attr name="position" intvalue="0"/>
            <attr name="advancedOptionsFolder" stringvalue="OptionsDialog/BookViewerOptionsCategory"/>
            <attr name="categoryName" bundlevalue="kiyut.alkitab.options.Bundle#BookViewerOptionsCategory.Name"/>
            <attr name="iconBase" bundlevalue="kiyut.alkitab.options.Bundle#BookViewerOptionsCategory.IconBase"/>
            <attr name="instanceCreate" methodvalue="org.netbeans.spi.options.OptionsCategory.createCategory"/>
            <attr name="keywords" bundlevalue="kiyut.alkitab.options.Bundle#BookViewerOptionsCategory.Keywords"/>
            <attr name="keywordsCategory" bundlevalue="kiyut.alkitab.options.Bundle#BookViewerOptionsCategory.KeywordsCategory"/>
            <attr name="title" bundlevalue="kiyut.alkitab.options.Bundle#BookViewerOptionsCategory.Title"/>
        </file>
    </folder>
    <folder name="Services">
        <folder name="Alkitab">
            <file name="CoreBookViewProvider.instance">
                <attr name="instanceClass" stringvalue="kiyut.alkitab.core.CoreBookViewProvider"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="BookshelfTopComponent.settings" url="windows/BookshelfTopComponent.settings"/>
            <file name="BookNavigatorTopComponent.settings" url="windows/BookNavigatorTopComponent.settings"/>
            <file name="DefinitionsTopComponent.settings" url="windows/DefinitionsTopComponent.settings"/>
            <file name="DailyDevotionsTopComponent.settings" url="windows/DailyDevotionsTopComponent.settings"/>
            <file name="GlobalHistoryTopComponent.settings" url="windows/GlobalHistoryTopComponent.settings"/>
        </folder> 
        <folder name="Modes">
            <folder name="explorer">
                <file name="BookshelfTopComponent.wstcref" url="windows/BookshelfTopComponent.wstcref">
                    <attr name="position" intvalue="100"/>
                 </file>
                <file name="BookNavigatorTopComponent.wstcref" url="windows/BookNavigatorTopComponent.wstcref">
                    <attr name="position" intvalue="110"/>
                 </file>
                 <file name="GlobalHistoryTopComponent.wstcref" url="windows/GlobalHistoryTopComponent.wstcref">
                    <attr name="position" intvalue="120"/>
                 </file>
            </folder>
            <folder name="output">
                <file name="DefinitionsTopComponent.wstcref" url="windows/DefinitionsTopComponent.wstcref">
                    <attr name="position" intvalue="200"/>
                 </file>
                <file name="DailyDevotionsTopComponent.wstcref" url="windows/DailyDevotionsTopComponent.wstcref">
                    <attr name="position" intvalue="210"/>
                </file>
            </folder>
        </folder>
    </folder>
    <folder name="Alkitab">
        <folder name="Bookshelf">
            <folder name="PopupMenu">
                <file name="kiyut-alkitab-actions-BookshelfOpenSelectedAction.instance">
                    <attr name="instanceClass" stringvalue="kiyut.alkitab.actions.BookshelfOpenSelectedAction"/>
                    <attr name="position" intvalue="10"/>
                </file>
                <file name="kiyut-alkitab-actions-BookshelfChangeFontAction.instance">
                    <attr name="instanceClass" stringvalue="kiyut.alkitab.actions.BookshelfChangeFontAction"/>
                    <attr name="position" intvalue="100"/>
                </file>
                <file name="kiyut-alkitab-actions-BookshelfIndexerAction.instance">
                    <attr name="instanceClass" stringvalue="kiyut.alkitab.actions.BookshelfIndexerAction"/>
                    <attr name="position" intvalue="500"/>
                </file>
                <file name="kiyut-alkitab-actions-BookshelfBookPropertiesAction.instance">
                    <attr name="instanceClass" stringvalue="kiyut.alkitab.actions.BookshelfBookPropertiesAction"/>
                    <attr name="position" intvalue="1000"/>
                </file>
            </folder>
        </folder>
    </folder>
</filesystem>
